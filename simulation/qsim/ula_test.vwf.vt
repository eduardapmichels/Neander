`timescale 1 ps/ 1 ps
module ULA_vlg_sample_tst(
	Sel,
	X,
	Y,
	sampler_tx
);
input [2:0] Sel;
input [7:0] X;
input [7:0] Y;
output sampler_tx;

reg sample;
time current_time;
always @(Sel or X or Y)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

